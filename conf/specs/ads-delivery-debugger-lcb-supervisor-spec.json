{
  "type": "kafka",
  "ioConfig": {
    "type": "kafka",
    "consumerProperties": {
      "bootstrap.servers": {
        "type": "serverset",
        "file": "/var/serverset/discovery.m10nkafka02.prod"
      }
    },
    "topic": "delivery_debugger_lcb",
    "lateMessageRejectionPeriod": "PT79200S",
    "useEarliestOffset": false,
    "taskCount": 5
  },
  "tuningConfig": {
    "type": "kafka",
    "resetOffsetAutomatically": true
  },
  "dataSchema": {
    "dataSource": "ad_delivery_history_lcb",
    "granularitySpec": {
      "type": "uniform",
      "segmentGranularity": "HOUR",
      "queryGranularity": "MINUTE",
      "rollup": false
    },
    "parser": {
      "type": "string",
      "parseSpec": {
        "format": "json",
        "timestampSpec": {
          "column": "timestampMs",
          "format": "millis"
        },
        "dimensionsSpec": {
          "dimensions": [
            {
              "type": "long",
              "name": "currentSpend"
            },
            {
              "type": "float",
              "name": "discountScore"
            },
            {
              "type": "long",
              "name": "entityID"
            },
            {
              "type": "float",
              "name": "previousDiscountScore"
            },
            "segment",
            "source",
            {
              "type": "long",
              "name": "targetSpend"
            }
          ]
        }
      }
    }
  }
}