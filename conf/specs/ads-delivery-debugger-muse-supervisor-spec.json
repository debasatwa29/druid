{
	"type": "kafka",
	"dataSchema": {
		"dataSource": "ad_delivery_history_muse",
		"parser": {
			"type": "string",
			"parseSpec": {
				"format": "json",
				"timestampSpec": {
					"column": "timestampMs",
					"format": "millis"
				},
				"dimensionsSpec": {
					"dimensions": [
						"gAdGroupID",
						"gCampaignID",
						"gAdvertiserID",
						"viewType",
						"creativeType",
						"candidateSource",
						"muse",
						"trimmedStage"
					]
				}
			}
		},
		"metricsSpec": [
			{
				"type": "longSum",
				"name": "count",
				"fieldName": "count"
			}
		],
		"granularitySpec": {
			"type": "uniform",
			"segmentGranularity": "hour",
			"queryGranularity": "fifteen_minute",
			"rollup": true
		}
	},
	"tuningConfig": {
		"type": "kafka",
		"maxRowsPerSegment": 15000000,
		"resetOffsetAutomatically": "true"
	},
	"ioConfig": {
		"topic": "delivery_debugger_muse",
		"consumerProperties": {
			"bootstrap.servers": "m10nkafka02160:9092,m10nkafka02159:9092,m10nkafka02158:9092,m10nkafka02157:9092,m10nkafka02156:9092,m10nkafka02155:9092,m10nkafka02154:9092,m10nkafka02153:9092,m10nkafka02152:9092,m10nkafka02151:9092"
		},
		"taskCount": 5,
		"replicas": 1,
		"taskDuration": "PT1H",
		"useEarliestOffset": "true"
	}
}