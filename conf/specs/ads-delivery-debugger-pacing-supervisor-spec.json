{
  "type": "kafka",
  "ioConfig": {
    "type": "kafka",
    "consumerProperties": {
      "bootstrap.servers": {
        "type": "serverset",
        "file": "/var/serverset/discovery.m10nkafka02.prod"
      }
    },
    "topic": "delivery_debugger_pacing",
    "useEarliestOffset": true,
    "taskCount": 5
  },
  "tuningConfig": {
    "type": "kafka",
    "maxRowsPerSegment": 15000000,
    "resetOffsetAutomatically": true
  },
  "dataSchema": {
    "dataSource": "ad_delivery_history_pacing",
    "granularitySpec": {
      "type": "uniform",
      "segmentGranularity": "HOUR",
      "queryGranularity": "MINUTE",
      "rollup": false
    },
    "parser": {
      "type": "string",
      "parseSpec": {
        "format": "json",
        "timestampSpec": {
          "column": "timestampMs",
          "format": "millis"
        },
        "dimensionsSpec": {
          "dimensions": [
            "segment",
            "source",
            "entityID",
            {
              "name": "currentSpend",
              "type": "float"
            },
            {
              "name": "targetSpend",
              "type": "float"
            },
            {
              "name": "pacingScore",
              "type": "float"
            }
          ]
        }
      }
    }
  }
}