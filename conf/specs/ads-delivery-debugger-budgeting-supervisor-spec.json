{
  "type": "kafka",
  "ioConfig": {
    "type": "kafka",
    "topic": "delivery_debugger_budgeting",
    "consumerProperties": {
      "bootstrap.servers": "m10nkafka02160:9092,m10nkafka02159:9092,m10nkafka02158:9092,m10nkafka02157:9092,m10nkafka02156:9092,m10nkafka02155:9092,m10nkafka02154:9092,m10nkafka02153:9092,m10nkafka02152:9092,m10nkafka02151:9092"
    },
    "taskCount": 5
  },
  "tuningConfig": {
    "type": "kafka",
    "maxRowsPerSegment": 15000000,
    "resetOffsetAutomatically": true
  },
  "dataSchema": {
    "dataSource": "ad_delivery_history_budgeting",
    "granularitySpec": {
      "type": "uniform",
      "segmentGranularity": "HOUR",
      "queryGranularity": "MINUTE",
      "rollup": false
    },
    "parser": {
      "type": "string",
      "parseSpec": {
        "format": "json",
        "timestampSpec": {
          "column": "timestampSec",
          "format": "posix"
        },
        "dimensionsSpec": {
          "dimensions": [
            "adgroupOverbudget",
            "advertiserOverbudget",
            "campaignOverbudget",
            {
              "type": "long",
              "name": "currentDailySpend"
            },
            {
              "type": "long",
              "name": "entityID"
            },
            "source",
            {
              "name": "effectiveDailyBudget",
              "type": "float"
            }
          ]
        }
      }
    }
  }
}