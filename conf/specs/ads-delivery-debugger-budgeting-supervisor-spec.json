{
  "type": "kafka",
  "ioConfig": {
    "type": "kafka",
    "topic": "delivery_debugger_budgeting",
    "consumerProperties": {
      "bootstrap.servers": {
        "type": "serverset",
        "file": "/var/serverset/discovery.m10nkafka02.prod"
      }
    },
    "taskCount": 5
  },
  "tuningConfig": {
    "type": "kafka",
    "maxRowsPerSegment": 15000000,
    "resetOffsetAutomatically": true
  },
  "dataSchema": {
    "dataSource": "ad_delivery_history_budgeting",
    "granularitySpec": {
      "type": "uniform",
      "segmentGranularity": "HOUR",
      "queryGranularity": "MINUTE",
      "rollup": false
    },
    "parser": {
      "type": "string",
      "parseSpec": {
        "format": "json",
        "timestampSpec": {
          "column": "timestampSec",
          "format": "posix"
        },
        "dimensionsSpec": {
          "dimensions": [
            "adgroupOverbudget",
            "advertiserOverbudget",
            "campaignOverbudget",
            {
              "type": "long",
              "name": "currentDailySpend"
            },
            {
              "type": "long",
              "name": "entityID"
            },
            "service",
            {
              "name": "effectiveDailyBudget",
              "type": "float"
            }
          ]
        }
      }
    }
  }
}